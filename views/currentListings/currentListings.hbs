<div class="search-bar center-content">
  <form class=search id="locations" name="searchForm" onsubmit="return validateForm()">
    <input class="inputFullWidth" type="text" name="origin" id="origin" placeholder="origin">
    <input class="inputFullWidth" type="text" name="destination" id="destination" placeholder="destination">
    <button name="searchButton" id="searchButton">Find</button>
    <p id="alert"></p>
  </form>
</div>
 <div class="current-listings-container">
  <div class="container u-cf">

<section id="newRides">
  <div class="container">
    {{#each listing}}
      <div class="listing">
        <h3>departure date: {{this.departureDate}}
          departure time: {{this.departureTime}}</h3>
        <h4>{{this.origin}} - {{this.destination}}</h4>
        <p>{{this.name}}</p>
        <image src ='{{this.urlImage}}' height='200'>
        <a href="#" id="{{this.listingID}}" class="seeMore">See more</a>
      <button class="listing-button"><a href="#" id="{{this.listingID}}" class="seeMore">See more</a></button>
        </div>
        {{/each}}
      </div>
</section>
    <script src="/javascript/formValidation.js"></script>
    <script src="/javascript/bundle.js"></script>
  </div>
</div> <!-- end of current-listings-container -->
