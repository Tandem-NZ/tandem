<header>
  <div id="logo">
    <a href="/">tandem.</a>
  </div>
  <nav class="primary-nav">
    <ul>
      <li class="active">
        <a href="/howItWorks">How it works</a>
      </li>
      <li class="active">
        <a href="/createListing">Create a Listing</a>
      </li>
      <li>
        <a href="/profile">Update your profile</a>
      </li>
      <li>
        <a href="/signin">Sign in</a>
      </li>
      <li>
        <a href="/signup" id="signup-btn-nav">Sign up</a>
      </li>
    </ul><!-- phone nav button -->
    <button class="nav-button">toggle menu</button>
  </nav>
</header><!-- end of header -->
<div class="current-listings-container u-full-width">
<div class="search-bar center-content">
    <form class="search u-full-width" id="locations" name="searchForm"
    onsubmit="return validateForm()">
    <input class="inputFullWidth" id="origin" name="origin"
    placeholder="origin" type="text"> <i class="fa fa-long-arrow-right arrow-nav-bar" aria-hidden="true"></i> <input class="inputFullWidth"
    id="destination" name="destination" placeholder="destination"
    type="text"> <button id="searchButton" name=
    "searchButton">Find</button>
    <p id="alert"></p>
  </form>
</div>
<div class="container u-cf">
  <section id="newRides">
    <div class="container">
      {{#each listing}}
      <div class="listing">
        <h3>departure date: {{this.departureDate}} departure
          time: {{this.departureTime}}</h3>
          <h4>{{this.origin}} - {{this.destination}}</h4>
          <p>{{this.name}}</p><img alt="profile pic" height='200'
          src='{{this.urlImage}}'> <button class=
          "listing-button"><a class="seeMore" href="#" id=
          "{{this.listingID}}">See more</a></button>
        </div>{{/each}}
      </div>
    </section>
    <script src="/javascript/formValidation.js">
    </script>
  </div>
</div>