 <div class="current-listings-container">

<div class="search-bar center-content container u-cf" id="testing-two">
  <form class=search id="locations" name="searchForm" onsubmit="return validateForm()">
  <input class="inputFullWidth" type="text" name="origin" id="origin" placeholder="origin"></input>
  <input class="inputFullWidth" type="text" name="destination" id="destination" placeholder="destination"></input>
  <button name="searchButton" id="searchButton">Find</button>
  <p id="alert"></p>
  </form>
</div>


  <div class="container u-cf" id="testing">
    <section id="newRides">
      <div class="numberOfRides">
        <h4>{{numberOfRides}} rides from {{origin}} to {{destination}}</h4>
      </div>

      <div>
        {{#each listing}}
        <div class="listing">
          <h6>{{this.dateTime}} - {{this.time}}</h6>
          <h4>{{this.origin}} - {{this.destination}}</h4>
          <p>{{this.name}}</p>
          <button class="listing-button"><a href="#" id="{{this.listingID}}" class="seeMore">See more</a></button>
        </div>
        {{/each}}
      </div>
    </section>
    <script src="/javascript/formValidation.js"></script>
    <script src="/javascript/bundle.js"></script>
  </div>
</div> <!-- end of current-listings-container -->

