<div class="current-listings-container u-full-width">
  <div class="search-bar center-content u-cf u-full-width">
    <form class="search u-full-width" id="locations" name="searchForm" onsubmit="return validateForm()">
      <input class="inputFullWidth" type="text" name="origin" id="origin" placeholder="origin"></input>
      <input class="inputFullWidth" type="text" name="destination" id="destination" placeholder="destination"></input>
      <button name="searchButton" id="searchButton">Find</button>
      <p id="alert"></p>
    </form>
  </div>
  <div class="container u-cf">

    <section id="newRides">
      <div class="container">
        {{#each listing}}

        <div class="listing">
          <h3>departure date: {{this.departureDate}}
            departure time: {{this.departureTime}}</h3>
          <h4>{{this.origin}} - {{this.destination}}</h4>
          <p>{{this.name}}</p>
          <image src ='{{this.urlImage}}' height='200'>
          <button class="listing-button"><a href="#" id="{{this.listingID}}" class="seeMore">See more</a></button>
        </div>

        {{/each}}
      </div>
    </section>
<script src="/javascript/formValidation.js"></script>
  </div>
</div>
